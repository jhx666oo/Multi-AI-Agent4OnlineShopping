{
  "version": "1.0.0",
  "description": "属性定义 - 跨境电商核心属性",
  "attributes": [
    {
      "id": "attr_plug_type",
      "name": {"en": "Plug Type", "zh": "插头类型"},
      "type": "enum",
      "values": ["US", "EU", "UK", "AU", "CN", "Universal"],
      "applies_to": ["cat_electronics", "cat_kitchen_appliances", "cat_lighting"],
      "is_variant_axis": true,
      "affects_compliance": true
    },
    {
      "id": "attr_voltage",
      "name": {"en": "Voltage", "zh": "电压"},
      "type": "enum",
      "values": ["110V", "220V", "110-240V"],
      "applies_to": ["cat_electronics", "cat_kitchen_appliances"],
      "is_variant_axis": true,
      "affects_compliance": true
    },
    {
      "id": "attr_color",
      "name": {"en": "Color", "zh": "颜色"},
      "type": "enum",
      "values": ["Black", "White", "Silver", "Gold", "Blue", "Red", "Green", "Pink", "Purple", "Gray", "Brown", "Orange", "Yellow", "Multicolor"],
      "applies_to": ["*"],
      "is_variant_axis": true,
      "affects_compliance": false
    },
    {
      "id": "attr_size",
      "name": {"en": "Size", "zh": "尺寸"},
      "type": "enum",
