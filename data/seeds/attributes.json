      "unit": "mm",
      "applies_to": ["*"],
      "is_variant_axis": false,
      "affects_compliance": true
    },
    {
      "id": "attr_certification",
      "name": {"en": "Certifications", "zh": "认证"},
      "type": "list",
      "values": ["CE", "FCC", "RoHS", "UL", "ETL", "PSE", "CCC", "KC", "ASTM", "CPSC", "EN71"],
      "applies_to": ["*"],
      "is_variant_axis": false,
      "affects_compliance": true
